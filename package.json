{
  "name": "Ecommerce-TypeScript",
  "version": "1.0.0",
  "description": "This is a ecommerce created with typescript",
  "main": "index.js",
  "scripts": {
    "build:next": "next build",
    "build:server": "tsc --project tsconfig.server.json",
