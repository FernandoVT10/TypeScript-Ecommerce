{
    "compilerOptions": {
        "module": "commonjs",
        "outDir": "dist",
        "noEmit": false,
        "allowJs": true,
        "esModuleInterop": true,
        "baseUrl": "server",
        "downlevelIteration": true,
	"typeRoots": ["./types", "node_modules/@types"]
    },
    "include": ["server"],
    "exclude": [
        "node_modules",
        "src",
	"server/tests/",
	"server/**/__tests__/"
    ]
}
