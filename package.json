{
  "name": "Ecommerce-TypeScript",
  "version": "1.0.0",
  "description": "This is a ecommerce created with typescript",
  "main": "index.js",
  "scripts": {
    "build:next": "next build",
    "build:server": "tsc --project tsconfig.server.json",
    "build": "npm run build:next && npm run build:server",
    "dev": "nodemon ./server/index.ts --exec \"ts-node --project tsconfig.server.json\"",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "mongoose": "^5.10.8",
    "next": "^9.5.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.14",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^14.11.5",
    "@types/react": "^16.9.51",
    "@types/react-dom": "^16.9.8",
    "@types/supertest": "^2.0.10",
    "@types/testing-library__jest-dom": "^5.9.4",
    "@types/testing-library__react": "^10.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.5.2",
    "jest-fetch-mock": "^3.0.3",
    "nodemon": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sass": "^1.27.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
